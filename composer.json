{
    "name": "avfigueredo/feedvel",
    "description": "Package to obtain the feed rss",
    "keywords": [
        "avfigueredo",
        "laravel",
        "feedvel"
    ],
    "homepage": "https://github.com/avfigueredo/feedvel",
    "license": "MIT",
    "authors": [
        {
            "name": "Andre Figueredo",
            "email": "andrevfigueredo@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "beberlei/assert": "^3.3",
        "illuminate/contracts": "^8.37",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "pestphp/pest": "^1.4",
        "pestphp/pest-plugin-laravel": "^1.0",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Avfigueredo\\Feedvel\\": "src",
            "Avfigueredo\\Feedvel\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Avfigueredo\\Feedvel\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Avfigueredo\\Feedvel\\FeedvelServiceProvider"
            ],
            "aliases": {
                "Feedvel": "Avfigueredo\\Feedvel\\FeedvelFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
